{
  "name": "sebastienheyd/hidden-captcha",
  "description": "Fully hidden captcha for Laravel without reCaptcha",
  "keywords": [ "laravel", "captcha", "hidden", "invisible" ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastien HEYD",
      "email": "contact@sheyd.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "SebastienHeyd\\HiddenCaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SebastienHeyd\\HiddenCaptcha\\Tests\\": "tests/",
      "App\\Http\\Controllers\\": "tests/"
    }
  },
  "require": {
    "php" : ">=5.6.4",
    "ext-json": "*",
    "laravel/framework": "^5.4|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "squizlabs/php_codesniffer": "^3.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
    "doctrine/dbal": "^2.9"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SebastienHeyd\\HiddenCaptcha\\HiddenCaptchaServiceProvider"
      ],
      "aliases": {
        "HiddenCaptcha": "SebastienHeyd\\HiddenCaptcha\\Facades\\HiddenCaptcha"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
